{
  "name": "@wdio/jsonwp-service",
  "version": "0.1.0",
  "description": "A WebdriverIO service that enables JSONWireProtocol commands for your session.",
  "homepage": "https://webdriver.io",
  "license": "MIT",
  "author": {
    "name": "Christian Bromann",
    "email": "mail@bromann.dev",
    "url": "https://bromann.dev"
  },
  "keywords": [
    "webdriver",
    "webdriverio",
    "wdio",
    "wdio-service"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/wdio-jsonwp-service.git"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": [
      {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    ]
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "prepare": "husky install",
    "install": "test -f ./dist/install.js && node ./dist/install.js || echo \"Skipping install, project not build!\"",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "test:unit": "vitest --run",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/node": "^20.14.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitest/coverage-v8": "^2.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.0.11",
    "npm-run-all2": "^6.2.0",
    "release-it": "^17.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^2.0.2",
    "wait-port": "^1.1.0",
    "webdriverio": "*"
  },
  "dependencies": {
    "@wdio/types": "^8.39.0"
  },
  "peerDependencies": {
    "@wdio/protocols": "^8.38.0",
    "webdriver": "^8.x",
    "webdriverio": "^8.x"
  },
  "peerDependenciesMeta": {
    "webdriverio": {
      "optional": true
    },
    "webdriver": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
